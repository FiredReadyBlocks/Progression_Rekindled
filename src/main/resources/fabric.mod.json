{
	"schemaVersion": 1,
	"id": "progression-rekindled",
	"version": "${version}",
	"name": "Progression Rekindled",
	"description": "A mod aiming to smoothen out Minecraft's progression, while adding value to previously useless materials. Actually useful Copper equipment, more incentives to upgrade to Netherite, Rosegold, reworked Shields, and more!",
	"authors": [
		"FiredReadyBlocks"
	],
	"contact": {
		"homepage": "https://www.youtube.com/@firedreadyblocks7427",
		"sources": "https://github.com/FiredReadyBlocks/Progression_Rekindled"
	},
	"license": "MIT",
	"icon": "assets/progression-rekindled/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"fireds.progressionrekindled.ProgressionRekindled"
		],
		"fabric-datagen": [
			"fireds.progressionrekindled.ProgressionRekindledDataGenerator"
		],
		"client": [
			"fireds.progressionrekindled.ProgressionRekindledClient"
		]
	},
	"mixins": [
		"progression-rekindled.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}